<% cards.each  do |card| %>
  <% cache [current_user, card] do %>
    <% if card.is_a? Project %>
      <%= render partial: 'projects/index/project_card', locals: { project: card } %>
    <% else %>
      <%= render partial: 'projects/index/folder_card', locals: { folder: card } %>
    <% end %>
  <% end %>
<% end %>
