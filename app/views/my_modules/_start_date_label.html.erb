<span class="start-date-label">
  <% if my_module.started_on.present? %>
    <strong><%= l(my_module.started_on, format: :full) %></strong>
  <% elsif start_date_editable %>
    <span class="empty-label">
      <%= t('my_modules.details.no_start_date_placeholder') %>
    </span>
  <% end %>
</span>
