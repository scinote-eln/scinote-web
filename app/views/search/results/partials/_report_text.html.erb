<% query ||= nil %>
<% text = query.present? ? highlight(report.name, query.strip.split(/\s+/)) : report.name %>

<% if can_read_project?(report.project) %>
  <%= route_to_other_team edit_project_report_path(report.project, report),
                          report.project.team,
                          text %>
<% else %>
  <%= text %>
<% end %>
