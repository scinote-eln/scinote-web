<ul class="sidebar-branch">
  <li class="sidebar-leaf">
    <%= link_to t('sidebar.my_modules.back_button'), project_url(experiment.project), class: 'sidebar-link back-button' %>
  </li>
  <% my_modules&.readable_by_user(current_user)&.each do |my_module| %>
    <li class="sidebar-leaf">
      <%= link_to my_module.name,
                  protocols_my_module_url(my_module),
                  title: my_module.name,
                  class: 'sidebar-link',
                  data: { module_id: my_module.id } %>
      <% if action_name == 'canvas' %>
        <i class="sn-icon sn-icon-location canvas-center-on" data-module-id="<%= my_module.id %>"></i>
      <% end %>
    </li>
  <% end %>
  <li class="sidebar-leaf">
    <%= link_to my_modules_experiment_path(experiment, view_mode: :archived), class: 'sidebar-link' do %>
      <i class="sn-icon sn-icon-archive"></i>&nbsp;
      <%= t('sidebar.my_modules.archived_button') %>
    <% end%>
  </li>
</ul>
