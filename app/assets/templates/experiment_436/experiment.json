            {
              "step": {
                "id": 4438,
                "name": "Centrifugation",
                "description": "<!DOCTYPE html PUBLIC \"-//W3C//DTD HTML 4.0 Transitional//EN\" \"http://www.w3.org/TR/REC-html40/loose.dtd\">\n<html><body><p>Follow the steps for centrifugation below.</p></body></html>",
                "position": 2,
                "completed": false,
                "completed_on": null,
                "user_id": 202,
                "created_at": "2018-11-13T10:55:00.370Z",
